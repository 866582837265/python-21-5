m1=int(input())
m2=int(input())
m3=int(input())
avg=(sub1+sub2+sub3)/3
if(avg>=90):
    print("Grade: A")
elif(avg>=80 and avg<90):
    print("Grade: B")
elif(avg>=70 and avg<80):
    print("Grade: C")
elif(avg>=60 and avg<70):
    print("Grade: D")
elif(avg>=50 and avg<60):
    print("Grade: E")
else:
    print("F")
